    "eslint": "5.13.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
